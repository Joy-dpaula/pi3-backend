{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www.js",
    "dev": "node --env-file=.env --watch app.js",
    "prisma:pull": "npx prisma db pull --schema=./prisma/schema-production.prisma",
    "prisma:push": "npx prisma db push --schema=./prisma/schema-production.prisma",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema-production.prisma",

    "prisma:local:pull": "npx prisma db pull --schema=./prisma/schema-local.prisma",
    "prisma:local:push": "npx prisma db push --schema=./prisma/schema-local.prisma",
    "prisma:local:generate": "npx prisma generate --schema=./prisma/schema-local.prisma",
    
    "seed": "node prisma/seed.js",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.4",
    "redis": "^4.7.0",
    "stripe": "^16.9.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "axios": "^1.7.5",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.15",
    "prisma": "^5.19.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": "off"
    }
  }
}
